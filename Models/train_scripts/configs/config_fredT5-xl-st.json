{
    "format": "SpecTokens",
    "max_bundles": 5,
    "model": "ai-forever/FRED-T5-1.7B",
    "add_nl_token": false,
    "add_eos_token": true,
    "change_pad_to_eos": true,
    "shuffle_bundles": true,
    "save_folder": "fredT5-xl",
    "train": {
        "n_epochs": 10,
        "lr": 0.00005,
        "batch_size": 4,
        "weight_decay": 0.01,
        "scheduler": "cosine",
        "warmup_steps": 500
    },
    "eval": {
        "batch_size": 4,
        "show": 5
    },
    "lora": {
        "rank": 64
    }
}