{
    "format": "SpecTokens",
    "max_bundles": 10,
    "max_eval_size": 100,
    "singles_to_mults_ratio": 2,
    "clean_text": true,
    "model": "ai-forever/FRED-T5-large",
    "add_nl_token": false,
    "add_eos_token": true,
    "change_pad_to_eos": false,
    "shuffle_bundles": true,
    "save_folder": "fredT5-large-ratio2",
    "train": {
        "n_epochs": 10,
        "lr": 5e-05,
        "batch_size": 4,
        "weight_decay": 0.01,
        "scheduler": "cosine",
        "warmup_steps": 500
    },
    "eval": {
        "batch_size": 4,
        "show": 5
    }
}