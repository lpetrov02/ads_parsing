{
    "format": "LightTokens",
    "max_bundles": 10,
    "singles_to_mults_ratio": 2,
    "max_eval_size": 100,
    "model": "ai-forever/FRED-T5-1.7B",
    "add_nl_token": false,
    "add_eos_token": true,
    "add_lm_token": true,
    "change_pad_to_eos": false,
    "shuffle_bundles": true,
    "clean_text": true,
    "save_folder": "fredT5-xl-lt",
    "train": {
        "n_epochs": 10,
        "lr": 3e-04,
        "batch_size": 4,
        "weight_decay": 0.0,
        "scheduler": "cosine",
        "warmup_steps": 1000
    },
    "eval": {
        "batch_size": 4,
        "show": 5
    },
    "lora": {
        "rank": 64
    }
}