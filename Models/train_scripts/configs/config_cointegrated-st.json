{
    "format": "SpecTokens",
    "max_bundles": 5,
    "model": "cointegrated/ruT5-base",
    "add_nl_token": false,
    "add_eos_token": false,
    "change_pad_to_eos": true,
    "shuffle_bundles": true,
    "save_folder": "m-ruT5-base",
    "train": {
        "n_epochs": 10,
        "lr": 0.00005,
        "batch_size": 16,
        "weight_decay": 0.01,
        "scheduler": "cosine",
        "warmup_steps": 500
    },
    "eval": {
        "batch_size": 8,
        "show": 5
    }
}